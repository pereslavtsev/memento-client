{
  "name": "@mementoweb/client",
  "version": "1.0.0",
  "description": "Time Travel APIs NodeJS library with full support of the Memento protocol.",
  "main": "dist/main.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prebuild": "tsc --project tsconfig.typings.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "prepare": "husky install"
  },
  "author": {
    "name": "Vitaly Pereslavtsev",
    "email": "dev@pereslavtsev.com",
    "url": "https://www.pereslavtsev.com/"
  },
  "homepage": "https://github.com/pereslavtsev/memento-client#readme",
  "bugs": {
    "url": "https://github.com/pereslavtsev/memento-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pereslavtsev/memento-client.git"
  },
  "license": "ISC",
  "keywords": [
    "internet",
    "archive",
    "wayback",
    "machine",
    "memento",
    "client"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "class-transformer": "^0.4.0",
    "debug": "^4.3.2",
    "luxon": "^2.0.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.0.2",
    "@types/luxon": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.3.5",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
